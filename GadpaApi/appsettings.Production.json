{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "GadpaDebateApi": "Information",
      "GadpaDebateApi.Hubs": "Information",
      "Microsoft.AspNetCore.SignalR": "Warning"
    },
    "Console": {
      "IncludeScopes": true,
      "TimestampFormat": "yyyy-MM-dd HH:mm:ss "
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "Jwt": {
    "Key": "${JWT_SECRET_KEY}",
    "Issuer": "GadpaDebateApi",
    "Audience": "GadpaDebateUsers"
  },
  "CORS": {
    "AllowedOrigins": [
      "${FRONTEND_URL}"
    ]
  }
}