{
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": [
            "/images/*.{png,jpg,gif,ico}",
            "/css/*",
            "/js/*",
            "/api/*"
        ]
    },
    "routes": [
        {
            "route": "/api/*",
            "allowedRoles": [
                "anonymous"
            ]
        }
    ],
    "responseOverrides": {
        "404": {
            "rewrite": "/index.html",
            "statusCode": 200
        }
    },
    "globalHeaders": {
        "cache-control": "must-revalidate, max-age=3600"
    },
    "mimeTypes": {
        ".json": "application/json",
        ".js": "text/javascript",
        ".css": "text/css"
    }
}